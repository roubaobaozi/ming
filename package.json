{
  "name": "ming",
  "version": "0.3.0",
  "description": "The resumé of Ming, Front end Engineer based in Sydney, Australia",
  "repository": "https://github.com/roubaobaozi/ming.git",
  "author": "Ming",
  "license": "GNU AGPLv3",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vike dev\" \"npm run watch\"",
    "build": "npm run build:vike && npm run build:copy",
    "build:vike": "vike build",
    "build:copy": "cp ./.htaccess ./dist/client/.htaccess",
    "preview": "vike preview",
    "-": "",
    "lint": "concurrently \"npm run lint:*\"",
    "lint:js": "eslint . && echo '✓ lint:js'",
    "lint:css": "stylelint \"src/**/*.css\" \"src/**/*.module.css\" && echo '✓ lint:css'",
    "--": "",
    "test": "vitest",
    "---": "",
    "watch": "concurrently \"npm run watch:*\"",
    "watch:js": "nodemon --watch src --watch test --ext ts --ext tsx --delay 3 --exec npm run lint:js",
    "watch:css": "nodemon --watch src --ext css --delay 3 --exec npm run lint:css"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.4.1",
    "node-fetch": "^3.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "vike": "^0.4.229",
    "vike-react": "^0.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@qalisa/vike-plugin-sitemap": "^1.5.5",
    "@types/postcss-modules-values": "^4.0.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "concurrently": "^9.1.2",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "postcss-modules": "^6.0.1",
    "postcss-modules-values": "^4.0.0",
    "prettier": "^3.5.3",
    "stylelint": "^16.19.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard": "^38.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.5",
    "vite-plugin-stylelint": "^6.0.0",
    "vitest": "^3.1.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-arm64-gnu": "^4.44.0"
  }
}
